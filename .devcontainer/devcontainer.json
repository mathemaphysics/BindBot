{
	"name": "BindBot",
	"build":
	{
		"context": ".",
		"dockerfile": "Dockerfile",
		"args":
		{
			"BOTNAME": "BindBot", // Better make sure this matches the folder
			"DEVUSER": "devuser",
			"DEVUID": "1000",
			"DEVGID": "1000"
		}
	},
	"customizations": {
		"vscode": {
			"extensions": [
				"ms-python.python",
				"ms-python.debugpy",
				"donjayamanne.python-extension-pack",
				"donjayamanne.python-environment-manager",
				"ms-azuretools.vscode-docker"
			]
		}
	}
}